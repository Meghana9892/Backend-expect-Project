<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.project.bean">
    <class  name="TeacherSubjectClass" table="teach_sub_class">
        <id column="ID" name="id" type="int" >
        <generator class="increment"/> 
        </id>
        
        <property column="tname" name="tname" type="string" />
        <property column="sname" name="sname" type="string" />
        <property column="cname" name="cname" type="string" />
       
       
    </class>
</hibernate-mapping>